msgstr ""

#. TRANSLATORS: settings option
#: src/gui/widgets/tabs/setup_chat.cpp:137
msgid "Show chat history"
msgstr "Mostrar mensagens antigas"

#. TRANSLATORS: settings description
#: src/gui/widgets/tabs/setup_chat.cpp:139
